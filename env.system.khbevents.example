# Example .env for system.khbevents.com (production)
# Copy to .env on the server: cp env.system.khbevents.example .env
# Then set DB_PASSWORD and run: php artisan key:generate --force

APP_NAME="KHB Booths Booking System"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://system.khbevents.com
APP_TIMEZONE=Asia/Phnom_Penh

# cPanel MySQL credentials
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=khbevents_aebooths
DB_USERNAME=khbevents_admaebooths
DB_PASSWORD=

SESSION_DRIVER=file
SESSION_LIFETIME=120
LOG_CHANNEL=stack
LOG_LEVEL=error
